-----------------------------------------------------------------------------------------------    
for(int i=0;i<n;i++){
		    for(int j=0;j<i;j++){
		        System.out.print(" ");
		    }
		    
		    for(int k=1;k<=2*(n-i)-1;k++){
		        System.out.print("*");
		    }
		    System.out.println();
		}
